<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/main_grey">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/base_framelayout"
        app:layout_constraintStart_toStartOf="@id/guideline_start_32"
        app:layout_constraintEnd_toEndOf="@id/guideline_start_1248"
        app:layout_constraintTop_toTopOf="@id/guideline_top_32"
        app:layout_constraintBottom_toBottomOf="@id/guideline_top_768" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_start_32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="32px" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_start_1248"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="1248px" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_top_32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="32px" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_top_768"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="768px" />
</androidx.constraintlayout.widget.ConstraintLayout>