<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".pages.home.HomeActivity">


    <Button
        android:id="@+id/img_profile"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/background_blue_main_block"
        android:text="@string/profile"
        app:layout_constraintBottom_toBottomOf="@id/guideline_top_281"
        app:layout_constraintEnd_toEndOf="@id/guideline_start_576"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/guideline_top_17" />

    <Button
        android:id="@+id/img_health_data"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/background_blue_main_block"
        android:text="@string/health_data"
        app:layout_constraintBottom_toBottomOf="@id/guideline_top_281"
        app:layout_constraintEnd_toEndOf="@id/guideline_start_885"
        app:layout_constraintStart_toStartOf="@id/guideline_start_621"
        app:layout_constraintTop_toTopOf="@id/guideline_top_17" />

    <Button
        android:id="@+id/img_phone"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/background_blue_main_block"
        android:text="@string/phone"
        app:layout_constraintBottom_toBottomOf="@id/guideline_top_281"
        app:layout_constraintEnd_toEndOf="@id/guideline_start_1051"
        app:layout_constraintStart_toStartOf="@id/guideline_start_931"
        app:layout_constraintTop_toTopOf="@id/guideline_top_17" />

    <Button
        android:id="@+id/img_date"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/background_blue_main_block"
        android:text="@string/date"
        app:layout_constraintBottom_toBottomOf="@id/guideline_top_281"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/guideline_start_1096"
        app:layout_constraintTop_toTopOf="@id/guideline_top_17" />

    <Button
        android:id="@+id/img_telehealth"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="@id/guideline_start_24"
        app:layout_constraintEnd_toEndOf="@id/guideline_start_288"
        app:layout_constraintTop_toTopOf="@id/guideline_top_377"
        app:layout_constraintBottom_toBottomOf="@id/guideline_top_641"
        android:text="@string/tele_health"
        android:background="@drawable/background_blue_main_block"/>

    <Button
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/img_manualinput"
        app:layout_constraintStart_toStartOf="@id/guideline_start_326"
        app:layout_constraintEnd_toEndOf="@id/guideline_start_590"
        app:layout_constraintTop_toTopOf="@id/guideline_top_377"
        app:layout_constraintBottom_toBottomOf="@id/guideline_top_641"
        android:text="@string/manual_input"
        android:background="@drawable/background_blue_main_block"/>

    <Button
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/img_medication"
        app:layout_constraintStart_toStartOf="@id/guideline_start_627"
        app:layout_constraintEnd_toEndOf="@id/guideline_start_891"
        app:layout_constraintTop_toTopOf="@id/guideline_top_377"
        app:layout_constraintBottom_toBottomOf="@id/guideline_top_641"
        android:text="@string/medication"
        android:background="@drawable/background_blue_main_block"/>

    <Button
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/img_measurement"
        app:layout_constraintStart_toStartOf="@id/guideline_start_929"
        app:layout_constraintEnd_toEndOf="@id/guideline_start_1193"
        app:layout_constraintTop_toTopOf="@id/guideline_top_377"
        app:layout_constraintBottom_toBottomOf="@id/guideline_top_641"
        android:text="@string/measurement"
        android:background="@drawable/background_blue_main_block"/>



    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_start_24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="24px" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_start_288"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
