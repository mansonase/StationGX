        android:orientation="vertical"
        app:layout_constraintGuide_begin="621px" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_start_627"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="627px" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_start_885"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="885px" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_start_891"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="891px" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_start_929"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="929px" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_start_931"
        android:layout_width="wrap_content"
