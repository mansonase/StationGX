        app:layout_constraintGuide_begin="281px" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_top_335"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="335px" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_top_377"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="377px" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_top_641"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
