        android:orientation="vertical"
        app:layout_constraintGuide_begin="931px" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_start_1051"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="1051px" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_start_1096"
