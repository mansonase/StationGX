<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.stationgx">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/FullscreenTheme"
        android:name=".pages.StationApp">

        <activity android:name=".pages.home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".pages.phone.PhoneActivity"/>
        <activity android:name=".pages.healthdata.HealthDataActivity"/>
        <activity android:name=".pages.manuelinput.ManualInputActivity"/>
        <activity android:name=".pages.measurement.MeasurementActivity"/>
        <activity android:name=".pages.medication.MedicationActivity"/>
        <activity android:name=".pages.myprofile.MyProfileActivity"/>
        <activity android:name=".pages.telehealth.TeleHealthActivity"/>

    </application>

</manifest>